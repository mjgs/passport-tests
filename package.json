    "express-session": "^1.15.5",
    "http-errors": "^1.6.2",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
