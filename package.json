{
  "name": "passport-tests",
  "version": "1.0.0",
  "description": "Express app that implements just the passport basics on a minimal website and api",
  "main": "app.js",
  "scripts": {
    "start": "node bin/website",
    "test": "mocha test 2> /dev/null",
